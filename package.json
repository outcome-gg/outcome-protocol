{
  "name": "outcome-protocol",
  "type": "module",
  "version": "1.0.0",
  "description": "Outcome Protocol",
  "scripts": {
    "test:unit": "FORMAT=wasm32-unknown-emscripten2 WASM=./AOS-SQLITE.wasm node --test test/unit/**/*.test.js",
    "test:unit:agents": "FORMAT=wasm32-unknown-emscripten2 WASM=./AOS-SQLITE.wasm node --test test/unit/agents/*.test.js",
    "test:unit:agents:dataAgent": "FORMAT=wasm32-unknown-emscripten2 WASM=./AOS-SQLITE.wasm node --test --test test/unit/agents/dataAgent.unit.test.js",
    "test:unit:agents:resolutionAgent": "FORMAT=wasm32-unknown-emscripten2 WASM=./AOS-SQLITE.wasm node --test --test test/unit/agents/resolutionAgent.unit.test.js",
    "test:unit:core": "FORMAT=wasm32-unknown-emscripten2 WASM=./AOS-SQLITE.wasm node --test test/unit/core/*.test.js",
    "test:unit:core:dataIndex": "FORMAT=wasm32-unknown-emscripten2 WASM=./AOS-SQLITE.wasm node --test test/unit/core/dataIndex*.test.js",
    "test:unit:market": "FORMAT=wasm32-unknown-emscripten2 WASM=./AOS-SQLITE.wasm node --test test/unit/market/*.test.js",
    "test:unit:core:conditionalTokens": "FORMAT=wasm32-unknown-emscripten2 WASM=./AOS-SQLITE.wasm node --test test/unit/core/conditionalTokens.unit.test.js",
    "test:unit:oracles": "FORMAT=wasm32-unknown-emscripten2 WASM=./AOS-SQLITE.wasm node --test test/unit/oracles/*.test.js",
    "test:integration": "yarn && mocha 'test/integration/**/*.js' --timeout 30000",
    "test:integration:clean": "yarn && yarn test:deploy && mocha 'test/integration/**/*.js' --timeout 30000",
    "test:integration:agents": "yarn && mocha 'test/integration/agents/*.js' --timeout 30000",
    "test:integration:agents:clean": "yarn && yarn test:deploy && mocha 'test/integration/agents/*.js' --timeout 30000",
    "test:integration:core": "yarn && mocha 'test/integration/core/*.js' --timeout 30000",
    "test:integration:core:clean": "yarn && yarn test:deploy && mocha 'test/integration/core/*.js' --timeout 30000",
    "test:integration:conditionalTokens": "yarn && mocha 'test/integration/conditionalTokens/*.js' --timeout 30000",
    "test:integration:onditionalTokens:clean": "yarn && yarn test:deploy && mocha 'test/integration/conditionalTokens/*.js' --timeout 30000",
    "test:integration:core:dataIndex": "yarn && mocha 'test/integration/core/dataIndex*.js' --timeout 30000",
    "test:integration:core:dataIndex:clean": "yarn && yarn test:deploy && mocha 'test/integration/core/dataIndex*.js' --timeout 30000",
    "test:integration:core:marketFoundry": "yarn && mocha 'test/integration/core/marketFoundry*.js' --timeout 30000",
    "test:integration:core:marketFoundry:clean": "yarn && yarn test:deploy && mocha 'test/integration/core/marketFoundry*.js' --timeout 30000",
    "test:integration:market": "yarn && mocha 'test/integration/market/*.js' --timeout 30000",
    "test:integration:market:clean": "yarn && yarn test:deploy && mocha 'test/integration/market/*.js' --timeout 30000",
    "test:integration:market:amm": "yarn && mocha 'test/integration/market/amm*.js' --timeout 50000",
    "test:integration:market:amm:clean": "yarn && yarn test:deploy && mocha 'test/integration/market/amm*.js' --timeout 50000",
    "test:integration:market:dlob": "yarn && mocha 'test/integration/market/dlob*.js' --timeout 50000",
    "test:integration:market:dlob:clean": "yarn && yarn test:deploy && mocha 'test/integration/market/dlob*.js' --timeout 50000",
    "test:integration:oracles": "yarn && mocha 'test/integration/oracles/*.js' --timeout 30000",
    "test:integration:oracles:clean": "yarn && yarn test:deploy && mocha 'test/integration/oracles/*.js' --timeout 30000",
    "test:sqlite": "FORMAT=wasm32-unknown-emscripten2 WASM=./AOS-SQLITE.wasm node --test test/**/*.test.js",
    "test:deploy": "scripts/deploy.sh test",
    "prod:deploy": "scripts/deploy.sh prod"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@permaweb/ao-loader": "^0.0.26",
    "@permaweb/aoconnect": "^0.0.51",
    "axios": "^1.7.2",
    "chai": "^4.3.10",
    "dotenv": "^16.4.5",
    "keccak256": "^1.0.6",
    "mocha": "^10.2.0",
    "path": "^0.12.7",
    "yargs": "^17.2.1"
  },
  "engines": {
    "node": ">=22"
  }
}
